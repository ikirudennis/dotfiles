[ui]
editor = vim
username = dburke <dennisburke@prodigy.net>
ignore = ~/.hgconfig/.hgignore

[extensions]
bitbucketrepo = ~/.hgext/bbext/bitbucketrepo
hgpunchcard = ~/.hgext/hgpunchcard.py
flow = ~/.hgext/hgflow/src/hgflow.py
highlight=
hggit = 
color = 
mq = 
progress = 
hgk = 
fetch = 
pager = 
record = 
shelve = 


[merge-tools]
macvim.executable = /Applications/Vim/mvim
macvim.args = -f $base $local $other $output -c 'SpliceInit'
macvim.premerge = keep
macvim.priority = 1
macvim.gui = true

vim.executable = vim
vim.args = -f $base $local $other $output -c 'SpliceInit'
vim.premerge = keep
vim.priority = 1
vim.gui = false

[color]
status.modified = cyan
status.added = green
status.removed = red
status.deleted = yellow bold underline
status.unknown = magenta bold underline
status.ignored = black bold
diff.diffline = none
diff.extended = cyan
diff.file_a = red
diff.file_b = green
diff.hunk = yellow bold
diff.deleted = red
diff.inserted = green
diff.changed = white
diff.trailingwhitespace = white_background

[alias]
cm = commit -m

[hostfingerprints]
bitbucket.org = 45:ad:ae:1a:cf:0e:73:47:06:07:e0:88:f5:cc:10:e5:fa:1c:f7:99

[hgk]
path=~/.hgext/hgk

[pager]
pager = LESS="R" less

[web]
pygments_style = murphy

[flow]
autoshelve = yes
