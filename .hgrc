[ui]
editor = vim
username = dburke <dennisburke@prodigy.net>
ignore = ~/.hgconfig/.hgignore

[extensions]
bitbucketextension = ~/.hgext/bbext/bitbucketrepo
hgpunchcard = ~/.hgext/hgpunchcard.py
hgext.bookmarks = 
hggit = 
color = 
mq = 

[merge-tools]
macvim.executable = /Applications/Vim/mvim
macvim.args = -f $base $local $other $output -c 'ThreesomeInit'
macvim.premerge = keep
macvim.gui = true

vim.executable = vim
vim.args = -d $base $local $other $output
vim.checkchanged = true
vim.gui = false

[color]
status.modified = cyan
status.added = green
status.removed = red
status.deleted = yellow bold underline
status.unknown = magenta bold underline
status.ignored = black bold
diff.diffline = none
diff.extended = cyan
diff.file_a = red
diff.file_b = green
diff.hunk = yellow bold
diff.deleted = red
diff.inserted = green
diff.changed = white
diff.trailingwhitespace = white_background

[alias]
cm = commit -m

[hostfingerprints]
