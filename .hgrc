[ui]
editor = vim
username = dburke <dennisburke@prodigy.net>
ignore = ~/.hgconfig/.hgignore

[extensions]
bitbucketrepo = ~/.hgext/bbext/bitbucketrepo
hgpunchcard = ~/.hgext/hgpunchcard.py
highlight=
hggit = 
color = 
mq = 
progress = 
hgk = 
fetch = 
pager = 
record = 


[merge-tools]
macvim.executable = /Applications/Vim/mvim
macvim.args = -f $base $local $other $output -c 'SpliceInit'
macvim.premerge = keep
macvim.priority = 1
macvim.gui = true

vim.executable = vim
vim.args = -f $base $local $other $output -c 'SpliceInit'
vim.premerge = keep
vim.priority = 1
vim.gui = false

[color]
status.modified = cyan
status.added = green
status.removed = red
status.deleted = yellow bold underline
status.unknown = magenta bold underline
status.ignored = black bold
diff.diffline = none
diff.extended = cyan
diff.file_a = red
diff.file_b = green
diff.hunk = yellow bold
diff.deleted = red
diff.inserted = green
diff.changed = white
diff.trailingwhitespace = white_background

[alias]
cm = commit -m

[hostfingerprints]
bitbucket.org = 24:9c:45:8b:9c:aa:ba:55:4e:01:6d:58:ff:e4:28:7d:2a:14:ae:3b

[hgk]
path=~/.hgext/hgk

[pager]
pager = LESS="R" less

[web]
pygments_style = murphy
